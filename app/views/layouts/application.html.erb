<!DOCTYPE html>
<html>
<head>
  <title>Normative Press</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="nav">
  	<% if current_user %>
    	<%= link_to "Edit Profile", edit_user_path(current_user.id)%>
      <%= link_to "Logout", :logout%>
    <% else %>
			<%= link_to "Login", :login %>
    <% end %>
  </div>

	<div id="content">
		<%= yield %>
	</div>
</body>
</html>